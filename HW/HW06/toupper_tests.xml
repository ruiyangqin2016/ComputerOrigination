<test-suite>
    <test-case>
        <name>toUpper("test")</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-string>
                <address>STRING</address>
                <value>test</value>
            </test-string>
        </input>
        <output>
            <test-string condition="equals" points="3">
                <address>STRING</address>
                <value>TEST</value>
            </test-string>
        </output>
    </test-case>
