<?xml version="1.0"?>
<test-suite>

    <test-case>
        <name>lower("abxyz") = "abxyz"</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
    <true-traps>1</true-traps>
        <randomize>1</randomize>
        <input>
            <test-register><register>R1</register><value>x1111</value></test-register>
            <test-register><register>R2</register><value>x2222</value></test-register>
            <test-register><register>R3</register><value>x3333</value></test-register>
            <test-register><register>R4</register><value>x4444</value></test-register>
            <test-register><register>R5</register><value>x5555</value></test-register>
            <test-register><register>R6</register><value>x6666</value></test-register>
            <test-string><address>STRING</address><value>abxyz</value></test-string>
        </input>
        <output>
            <test-register><register>R1</register><value>x1111</value></test-register>
            <test-register><register>R2</register><value>x2222</value></test-register>
            <test-register><register>R3</register><value>x3333</value></test-register>
            <test-register><register>R4</register><value>x4444</value></test-register>
            <test-register><register>R5</register><value>x5555</value></test-register>
            <test-register><register>R6</register><value>x6666</value></test-register>
            <test-string><address>STRING</address><value>abxyz</value></test-string>
        </output>
    </test-case>

